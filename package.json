{
  "name": "tools",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.19",
    "classnames": "^2.2.6",
    "fetch-ponyfill": "^6.0.2",
    "filenamify-url": "^1.0.0",
    "generate-password": "^1.4.0",
    "glfx-es6": "0.0.3",
    "is-url": "^1.2.4",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-dropzone": "^6.1.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-unicorn": "^6.0.1",
    "jest-prop-type-error": "^1.1.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "^2.0.4",
    "react-test-renderer": "^16.5.2",
    "stylelint": "^9.6.0",
    "stylelint-config-idiomatic-sass": "^1.0.0",
    "stylelint-config-strict-scss": "^0.5.0",
    "stylelint-scss": "^3.3.1",
    "stylelint-webpack-plugin": "^0.10.5"
  },
  "scripts": {
    "lint": "run-p lint:*",
    "lint:js": "eslint --ext .js --ext .jsx --ignore-path .gitignore .",
    "lint:scss": "stylelint --ignore-path .gitignore --syntax scss '**/*.scss'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:react/recommended",
      "plugin:unicorn/recommended",
      "plugin:prettier/recommended",
      "prettier",
      "prettier/react",
      "prettier/unicorn"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": false
    },
    "overrides": [
      {
        "files": [
          "src/**/*.js"
        ],
        "rules": {
          "import/prefer-default-export": "off",
          "import/no-default-export": "error"
        }
      },
      {
        "files": [
          "src/**/*.jsx"
        ],
        "rules": {
          "unicorn/filename-case": [
            "error",
            {
              "case": "pascalCase"
            }
          ]
        }
      },
      {
        "files": [
          "src/setupTests.js",
          "src/**/*.spec.*"
        ],
        "env": {
          "jest": true
        },
        "rules": {
          "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": true
            }
          ]
        }
      },
      {
        "files": [
          "src/registerServiceWorker.jsx",
          "src/setupTests.js",
          "src/**/*.spec.jsx",
          "src/**/index.jsx"
        ],
        "rules": {
          "unicorn/filename-case": "off"
        }
      }
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-strict-scss",
      "stylelint-config-idiomatic-sass"
    ]
  }
}
