{
  "name": "tools",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.19",
    "classnames": "^2.2.6",
    "fetch-ponyfill": "^6.0.2",
    "filenamify-url": "^1.0.0",
    "generate-password": "^1.4.0",
    "glfx-es6": "0.0.3",
    "is-url": "^1.2.4",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-dropzone": "^6.1.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "react-scripts": "^2.0.4",
    "react-test-renderer": "^16.5.2",
    "sass-lint": "^1.12.1"
  },
  "scripts": {
    "lint": "run-p lint:*",
    "lint:js": "eslint --ext .js --ext .jsx --ignore-path .gitignore .",
    "lint:scss": "sass-lint -i 'build/**, node_modules/**' '**/*.scss' -v -q",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": false
    },
    "rules": {
      "object-curly-newline": [
        "error",
        {
          "ObjectExpression": {
            "minProperties": 6,
            "multiline": true,
            "consistent": true
          },
          "ObjectPattern": {
            "minProperties": 6,
            "multiline": true,
            "consistent": true
          },
          "ImportDeclaration": "never",
          "ExportDeclaration": {
            "minProperties": 6,
            "multiline": true,
            "consistent": true
          }
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "src/setupTests.js",
          "src/**/*.spec.*"
        ],
        "env": {
          "jest": true
        },
        "rules": {
          "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": true
            }
          ]
        }
      }
    ]
  }
}
